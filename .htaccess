RewriteEngine On

# Handle /c/chatId routes - redirect to c.html
RewriteRule ^c/([a-z0-9]+)/?$ c.html [L]

# Handle root chatbot access
RewriteRule ^chatbot/?$ chatbot.html [L]

# Handle other routes
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ index.html [L]
